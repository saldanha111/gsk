    /**
     * Set needTesoreriaValidation
     *
     * @param boolean $needTesoreriaValidation
     * @return InstanciasWorkflows
     */
    public function setNeedTesoreriaValidation($needTesoreriaValidation)
    {
        $this->needTesoreriaValidation = $needTesoreriaValidation;

        return $this;
    }

    /**
     * Get needTesoreriaValidation
     *
     * @return boolean 
     */
    public function getNeedTesoreriaValidation()
    {
        return $this->needTesoreriaValidation;
    }
