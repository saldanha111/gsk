<table id="inProcessParcial" class="table table-striped">
    <thead>
    <tr>
        <th>Id</th>
        <th>Nº</th>
        <th>Plantilla</th>
        <th>Usuario</th>
        <th>Inicio</th>
        <th>Fin</th>
        <th>Acción</th>
        <th>Tiempo</th>
    </tr>
    </thead>
    {% if items is defined %}
        {% for item in items %}
            <tr>
                <td>{{item.id}}</td>
                <td>{{item.id_reg}}</td>
                <td>{{item.name}}</td>
                <td>{{item.creator}}</td>
                <td>{{item.entrada is defined and item.entrada is not empty ? item.entrada|date("d/m/Y H:i:s") : '' }}</td>
                <td>{{item.salida is defined and item.salida is not empty  ? item.salida|date("d/m/Y H:i:s") : '' }}</td>
                <td>{{item.accion|capitalize}}</td>
                <td>{{item.formatDuration}}</td>
            </tr>
        {% endfor %}
    {% endif %}
</table>
